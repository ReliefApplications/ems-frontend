  "license": "MIT",
  "scripts": {
    "check-i18n": "node .scripts/check-i18n",
    "semantic-release": "semantic-release",
    "prepare": "husky install",
    "lint": "npx nx run-many --target=lint --projects=safe,front-office,back-office,ui --verbose",
    "test": "npx nx run-many --target=test --projects=safe,front-office,back-office,ui --configuration=ci",
    "bundle:widgets": "set NODE_OPTIONS=--max_old_space_size=4096 && npx nx build web-widgets --configuration=oort-dev && node .scripts/build-widgets.js",
    "widgets:local": "set NODE_OPTIONS=--max_old_space_size=4096 && npx nx run web-widgets:serve",
    "back-office:local": "set NODE_OPTIONS=--max_old_space_size=4096 && npx nx serve back-office --configuration=oort-local",
    "back-office:dev": "set NODE_OPTIONS=--max_old_space_size=4096 && npx nx serve back-office --configuration=oort-dev",
    "back-office:azure": "set NODE_OPTIONS=--max_old_space_size=4096 && npx nx serve back-office --configuration=azure-dev",
    "back-office:local-prod": "set NODE_OPTIONS=--max_old_space_size=4096 && npx nx build back-office --configuration=oort-local"
  },
