{
  "name": "oort-front",
  "version": "1.3.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "bundle:widgets": "ng build --configuration=oort-dev --project=web-widgets && node .scripts/build-widgets.js",
    "compodoc:front": "npx compodoc -p tsconfig.json projects/front-office/src -d documentation/front-office",
    "compodoc:back": "npx compodoc -p tsconfig.json projects/back-office/src -d documentation/back-office",
    "compodoc:safe": "npx compodoc -p tsconfig.json projects/safe/src -d documentation/safe",
    "compodoc:widgets": "npx compodoc -p tsconfig.json projects/web-widgets/src -d documentation/web-widgets",
    "test": "ng test",
    "test:ci": "ng test",
    "lint": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng lint",
    "e2e": "ng e2e",
    "docs:json": "NODE_OPTIONS=--max-old-space-size=4096 npx compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "NODE_OPTIONS=--max-old-space-size=4096 npm run docs:json && NODE_OPTIONS=--max-old-space-size=4096 build-storybook",
    "prepare": "husky install",
    "check-i18n": "node .scripts/check-i18n",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "dependencies": {
