{
  "name": "oort-front",
  "version": "1.1.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "bundle:elem": "cat dist/web-element/runtime.js dist/web-element/polyfills.js dist/web-element/main.js > dist/web-element.js",
    "compodoc:front": "npx compodoc -p tsconfig.json projects/front-office/src -d documentation/front-office",
    "compodoc:back": "npx compodoc -p tsconfig.json projects/back-office/src -d documentation/back-office",
    "compodoc:safe": "npx compodoc -p tsconfig.json projects/safe/src -d documentation/safe",
    "compodoc:elem": "npx compodoc -p tsconfig.json projects/web-element/src -d documentation/web-element",
    "test": "ng test",
    "test:ci": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs:json": "npx compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "prepare": "husky install",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "release:alpha": "standard-version --release-as patch --prerelease alpha"
