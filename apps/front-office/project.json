{
  "name": "front-office",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/front-office/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/front-office",
        "index": "apps/front-office/src/index.html",
        "main": "apps/front-office/src/main.ts",
        "polyfills": ["apps/front-office/src/polyfills.ts"],
        "tsConfig": "apps/front-office/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "libs/safe/src/i18n/",
            "output": "/assets/i18n/"
          },
          {
            "glob": "**/*",
            "input": "assets/",
            "output": "/assets/"
          },
          "apps/front-office/src/favicon.ico",
          "apps/front-office/src/web.config",
          {
            "glob": "**/*",
            "input": "node_modules/tinymce",
            "output": "/tinymce/"
          },
          {
            "glob": "**/*",
            "input": "libs/safe/src/i18n/tinymce/",
            "output": "/tinymce/langs"
          },
          {
            "glob": "**/*",
            "input": "node_modules/@fortawesome/fontawesome-free/webfonts",
            "output": "/assets/webfonts/"
          },
          {
            "glob": "**/*",
            "input": "node_modules/leaflet/dist/images/",
            "output": "/"
          }
        ],
        "styles": [
          "apps/front-office/src/styles.scss",
          "libs/safe/src/lib/style/styles.scss",
          "node_modules/survey-angular/survey.min.css",
          "node_modules/survey-creator/survey-creator.min.css",
          "node_modules/leaflet/dist/leaflet.css",
          "node_modules/leaflet.markercluster/dist/MarkerCluster.css",
          "node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
          "node_modules/leaflet-fullscreen/dist/leaflet.fullscreen.css",
          "node_modules/leaflet-measure/dist/leaflet-measure.css",
          "node_modules/esri-leaflet-geocoder/dist/esri-leaflet-geocoder.css",
          "node_modules/@geoman-io/leaflet-geoman-free/dist/leaflet-geoman.css",
          "node_modules/leaflet-timedimension/dist/leaflet.timedimension.control.min.css"
        ],
        "scripts": [
          "node_modules/jquery/dist/jquery.js",
          "node_modules/tinymce/tinymce.js",
          "node_modules/tinymce/themes/silver/theme.js"
        ],
        "allowedCommonJsDependencies": [
          "minimatch",
          "hammerjs",
          "survey-creator",
          "survey-knockout",
          "subscriptions-transport-ws",
          "zen-observable",
          "lodash",
          "cronstrue",
          "cron-parser",
          "cron-validator",
          "localforage",
          "survey-angular",
          "fast-sha256",
          "leaflet",
          "esri-leaflet-geocoder",
          "esri-leaflet-vector",
          "leaflet.markercluster",
          "color",
          "esri-leaflet-heatmap",
          "esri-leaflet-renderers",
          "jsonpath"
        ]
      },
      "configurations": {
        "azure-prod": {
          "fileReplacements": [
            {
              "replace": "apps/front-office/src/environments/environment.ts",
              "with": "apps/front-office/src/environments/environment.azure.prod.ts"
            }
          ],
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "12mb",
              "maximumError": "15mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ],
          "outputHashing": "all",
          "stylePreprocessorOptions": {
            "includePaths": ["libs/safe/src/lib/style/themes/default"]
          }
        },
        "azure-uat": {
